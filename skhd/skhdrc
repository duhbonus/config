# changing window focus

# changing window focus within space
ctrl - j : yabai -m window --focus south
ctrl - k : yabai -m window --focus north
ctrl - h : yabai -m window --focus west
ctrl - l : yabai -m window --focus east

ctrl - b : yabai -m space --mirror x-axis
ctrl - f : yabai -m space --mirror y-axis

ctrl - r : yabai -m space --rotate 270
shift + ctrl - r : yabai -m space --rotate 90

ctrl - z : yabai -m window --toggle zoom-fullscreen
